<!DOCTYPE html>
<html>
<head>
  <title>Taskapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= image_tag "logo.jpg" %>
<% if notice %>

  <p class="alert alert-notice"><%= notice %></p>

	<% end %>

	<% if alert %>

	  <p class="alert alert-error"><%= alert %></p>

		<% end %>
<%= yield %>
<%= link_to "home", "/"%>

</body>
</html>
